 colors: ['Blue', 'White'],\n                sizes: ['XS', 'S', 'M', 'L', 'XL']\n            },\n            '1000009766': {\n                title: 'Long Sleeve Tennis Top',\n                price: '550,00€',\n                description: 'A long sleeve tennis top that provides comfort and style on the court.',\n                image: 'https:\/\/images.unsplash.com\/photo-1542272604-787c3835535d?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1026&q=80',\n                colors: ['White', 'Black'],\n                sizes: ['XS', 'S', 'M', 'L', 'XL']\n            },\n            'soft-winter-jacket': {\n                title: 'Soft Winter Jacket',\n                price: '500,00€',\n                description: 'A cozy winter jacket that keeps you warm in the coldest weather.',\n                image: 'https:\/\/images.unsplash.com\/photo-1551028719-00167b16eac5?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=735&q=80',\n                colors: ['Black'],\n                sizes: ['One Size']\n            }\n        };\n\n        let cart = [];\n\n        function createProductGrid() {\n            const productGrid = document.getElementById('product-grid');\n            Object.keys(products).forEach((productId, index) => {\n                if (productId !== 'soft-winter-jacket') {\n                    const product = products[productId];\n                    const productElement = document.createElement('div');\n                    productElement.className = 'product-item';\n                    productElement.innerHTML = `\n                        <img src=\"${product.image}\" alt=\"${product.title}\" class=\"product-image\">\n                        <div class=\"product-indicator\" style=\"top: ${30 + (index * 10)}%; left: ${20 + (index * 10)}%;\">+<\/div>\n                    `;\n                    productElement.querySelector('.product-indicator').addEventListener('click', () => openModal(productId));\n                    productGrid.appendChild(productElement);\n                }\n            });\n        }\n\n        function openModal(productId) {\n            const product = products[productId];\n            const modalContent = document.getElementById('modal-content');\n            modalContent.innerHTML = `\n                <img src=\"${product.image}\" alt=\"${product.title}\" class=\"product-modal-image\">\n                <div class=\"product-modal-details\">\n                    <h2 class=\"product-modal-title\">${product.title}<\/h2>\n                    <p class=\"product-modal-price\">${product.price}<\/p>\n                    <p class=\"product-modal-description\">${product.description}<\/p>\n                    <div>\n                        <p>Color<\/p>\n                        <div class=\"color-options\">\n                            ${product.colors.map((color, index) => `\n                                <div class=\"color-option ${index === 0 ? 'selected' : ''}\" data-color=\"${color}\">${color}<\/div>\n                            `).join('')}\n                        <\/div>\n                    <\/div>\n                    <div>\n                        <p>Size<\/p>\n                        <select class=\"size-select\">\n                            <option value=\"\">Choose your size<\/option>\n                            ${product.sizes.map(size => `<option value=\"${size}\">${size}<\/option>`).join('')}\n                        <\/select>\n                    <\/div>\n                    <div class=\"quantity-selector\">\n                        <button class=\"quantity-btn minus\">-<\/button>\n                        <span class=\"quantity-display\">1<\/span>\n                        <button class=\"quantity-btn plus\">+<\/button>\n                    <\/div>\n                    <button class=\"add-to-cart-btn\" data-product-id=\"${productId}\">\n                        ADD TO CART\n                        <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n                            <path d=\"M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2.293 2.293c-.63.63-.184 1.707.707 1.707H17m0 0a2 2 0 100 4 2 2 0 000-4zm-8 2a2 2 0 11-4 0 2 2 0 014 0z\"><\/path>\n                        <\/svg>\n                    <\/button>\n                <\/div>\n            `;\n\n            document.getElementById('modal-overlay').style.display = 'flex';\n            setupModalInteractions(productId);\n        }\n\n        function setupModalInteractions(productId) {\n            const colorOptions = document.querySelectorAll('.color-option');\n            const quantityDisplay = document.querySelector('.quantity-display');\n            const minusBtn = document.querySelector('.minus');\n            const plusBtn = document.querySelector('.plus');\n            const addToCartBtn = document.querySelector('.add-to-cart-btn');\n\n            colorOptions.forEach(option => {\n                option.addEventListener('click', () => {\n                    colorOptions.forEach(opt => opt.classList.remove('selected'));\n                    option.classList.add('selected');\n                });\n            });\n\n            minusBtn.addEventListener('click', () => {\n                let quantity = parseInt(quantityDisplay.textContent);\n                if (quantity > 1) {\n                    quantityDisplay.textContent = quantity - 1;\n                }\n            });\n\n            plusBtn.addEventListener('click', () => {\n                let quantity = parseInt(quantityDisplay.textContent);\n                quantityDisplay.textContent = quantity + 1;\n            });\n\n            addToCartBtn.addEventListener('click', () => {\n                const selectedColor = document.querySelector('.color-option.selected')?.dataset.color;\n                const selectedSize = document.querySelector('.size-select').value;\n                const quantity = parseInt(quantityDisplay.textContent);\n\n                if (!selectedColor || !selectedSize) {\n                    alert('Please select both color and size');\n                    return;\n                }\n\n                addToCart(productId, selectedColor, selectedSize, quantity);\n                document.getElementById('modal-overlay').style.display = 'none';\n            });\n        }\n\n        function addToCart(productId, color, size, quantity) {\n            const product = products[productId];\n            \n            const cartItem = {\n                id: productId,\n                title: product.title,\n                color: color,\n                size: size,\n                quantity: quantity,\n                price: product.price\n            };\n\n            const existingItemIndex = cart.findIndex(item => \n                item.id === productId && item.color === color && item.size === size\n            );\n\n            if (existingItemIndex !== -1) {\n                cart[existingItemIndex].quantity += quantity;\n            } else {\n                cart.push(cartItem);\n            }\n\n            \/\/ Check if we need to add Soft Winter Jacket\n            if (color === 'Black' && size === 'M') {\n                const softWinterJacket = {\n                    id: 'soft-winter-jacket',\n                    title: 'Soft Winter Jacket',\n                    color: 'Default',\n                    size: 'One Size',\n                    quantity: 1,\n                    price: '500,00€'\n                };\n                const existingSoftJacketIndex = cart.findIndex(item => item.id === 'soft-winter-jacket');\n                if (existingSoftJacketIndex !== -1) {\n                    cart[existingSoftJacketIndex].quantity += 1;\n                } else {\n                    cart.push(softWinterJacket);\n                }\n            }\n\n            updateCartCount();\n            updateCartSidebar();\n            showAddedToCartNotification();\n        }\n\n        function updateCartCount() {\n            const cartCount = cart.reduce((total, item) => total + item.quantity, 0);\n            document.getElementById('cart-count').textContent = cartCount;\n        }\n\n        function showAddedToCartNotification() {\n            const notification = document.getElementById('added-notification');\n            notification.style.display = 'block';\n            setTimeout(() => {\n                notification.style.display = 'none';\n            }, 3000);\n        }\n\n        function updateCartSidebar() {\n            const cartItems = document.getElementById('cart-items');\n            const cartTotal = document.getElementById('cart-total');\n            \n            cartItems.innerHTML = '';\n            let total = 0;\n\n            cart.forEach((item, index) => {\n                const itemElement = document.createElement('div');\n                itemElement.className = 'cart-item';\n                itemElement.innerHTML = `\n                    <div class=\"cart-item-details\">\n                        <div class=\"cart-item-title\">${item.title}<\/div>\n                        <div>Color: ${item.color}, Size: ${item.size}<\/div>\n                        <div class=\"cart-item-price\">${item.price}<\/div>\n                    <\/div>\n                    <div class=\"cart-item-quantity\">\n                        <button onclick=\"updateCartItemQuantity(${index}, -1)\">-<\/button>\n                        <span>${item.quantity}<\/span>\n                        <button onclick=\"updateCartItemQuantity(${index}, 1)\">+<\/button>\n                    <\/div>\n                    <button class=\"remove-btn\" onclick=\"removeCartItem(${index})\">\n                        <svg xmlns=\"http:\/\/www.w3.org\/2000\/svg\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\">\n                            <path d=\"M3 6h18\"><\/path>\n                            <path d=\"M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2\"><\/path>\n                            <line x1=\"10\" y1=\"11\" x2=\"10\" y2=\"17\"><\/line>\n                            <line x1=\"14\" y1=\"11\" x2=\"14\" y2=\"17\"><\/line>\n                        <\/svg>\n                        Remove\n                    <\/button>\n                `;\n                cartItems.appendChild(itemElement);\n\n                total += parseFloat(item.price.replace('€', '').replace(',', '.')) * item.quantity;\n            });\n\n            cartTotal.textContent = total.toFixed(2) + '€';\n        }\n\n        function updateCartItemQuantity(index, change) {\n            cart[index].quantity += change;\n            if (cart[index].quantity < 1) {\n                cart.splice(index, 1);\n            }\n            updateCartCount();\n            updateCartSidebar();\n        }\n\n        function removeCartItem(index) {\n            cart.splice(index, 1);\n            updateCartCount();\n            updateCartSidebar();\n        }\n\n        function clearCart() {\n            cart = [];\n            updateCartCount();\n            updateCartSidebar();\n        }\n        document.addEventListener('DOMContentLoaded', () => {\n            createProductGrid();\n\n            document.querySelector('.modal-close').addEventListener('click', () => {\n                document.getElementById('modal-overlay').style.display = 'none';\n            });\n\n            document.getElementById('modal-overlay').addEventListener('click', (e) => {\n                if (e.target === document.getElementById('modal-overlay')) {\n                    document.getElementById('modal-overlay').style.display = 'none';\n                }\n            });\n\n            document.getElementById('cart-icon-container').addEventListener('click', () => {\n                document.getElementById('cart-sidebar').classList.add('open');\n            });\n\n            document.querySelector('.cart-sidebar-close').addEventListener('click', () => {\n                document.getElementById('cart-sidebar').classList.remove('open');\n            });\n\n            document.getElementById('clear-cart').addEventListener('click', clearCart);\n\n            document.getElementById('checkout').addEventListener('click', () => {\n                alert('Checkout functionality not implemented in this demo.');\n            });\n        });\n    <\/script>\n<\/body>\n<\/html>","color_scheme":"","padding_top":40,"padding_bottom":52}}},"order":["custom_liquid_QL7PCH"]}